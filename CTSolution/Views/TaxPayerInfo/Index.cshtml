@model IEnumerable<CTSolution.Models.TaxPayerInfo>



<h5>အခွန်ထမ်းစာရင်း</h5>
<br />
<br />

<p>
    <a asp-action="Create">စာရင်းသွင်းရန်</a>
</p>

<table class="table">
    <thead>
        <tr>
            
            <th>
                @Html.DisplayNameFor(model => model.PersonName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NRCNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BusinessName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompanyName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BizRegNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TIN_Number)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BusinessType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CommercialLicense)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CommercialLicenseDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PhoneNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Township)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
               
                <td>
                    @Html.DisplayFor(modelItem => item.PersonName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NRCNo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BusinessName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CompanyName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BizRegNumber)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TIN_Number)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BusinessType)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CommercialLicense)
                </td>
                <td>
                    @item.CommercialLicenseDate.ToString("dd-MM-yyyy")
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PhoneNo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Address)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Township)
                </td>

                
                <td>

                    <a href="@Url.Action("Edit", "TaxPayerInfo", new { id = item.TaxPayerPkid })" class="btn btn-warning btn-sm"><i class="fas fa-edit"></i> Edit</a>
                    <a href="@Url.Action("Delete", "TaxPayerInfo", new { id = item.TaxPayerPkid })" class="btn btn-danger btn-sm"><i class="fas fa-trash"></i> Delete</a>
                    <a href="@Url.Action("Details", "TaxPayerInfo", new { id = item.TaxPayerPkid })" class="btn btn-info btn-sm"><i class="fas fa-info-circle"></i> Detail</a>
                </td>
            </tr>
        }
    </tbody>
</table>
